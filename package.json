{
  "name": "taxisure-backend",
  "module": "server.ts",
  "type": "module",
  "scripts": {
    "start": "bun run src/server.ts",
    "dev": "bun run --watch src/server.ts",
    "build": "bun build src/server.ts --outdir dist --target node",
    "lint": "bun run eslint src/**/*.ts",
    "format": "bun run prettier --write src/**/*.ts",
    "test": "bun run vitest",
    "db:clear": "bun run src/scripts/clear-database.ts",
    "db:reset": "bun run src/scripts/reset-database.ts",
    "db:full-reset": "bun run src/scripts/full-db-reset.ts",
    "delete-user": "bun run src/scripts/delete-user.ts"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bun": "latest",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.4.0",
    "@prisma/client": "^6.0.1",
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.17",
    "@types/cron": "^2.4.3",
    "@types/google-maps": "^4.0.0",
    "@types/multer": "^1.4.12",
    "@types/node-schedule": "^2.1.7",
    "@types/socket.io": "^3.0.2",
    "@types/uuid": "^10.0.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.5.1",
    "cors": "^2.8.5",
    "cron": "^4.1.4",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "firebase-admin": "^13.4.0",
    "jsonwebtoken": "^9.0.2",
    "mock-aws-s3": "^4.0.2",
    "multer": "^1.4.5-lts.1",
    "nock": "^13.5.6",
    "node-schedule": "^2.1.1",
    "prisma": "^6.0.1",
    "razorpay": "^2.9.5",
    "socket.io": "^4.8.1",
    "twilio": "^5.4.0",
    "uuid": "^11.0.3"
  }
}
